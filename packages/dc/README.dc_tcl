dc_trig_time(1,2)  -  (Default = -13.4) SC_P1 - DC_S2
dc_trig_time(1,3)  -  (Default = -6.4) SC_P1 - DC_S3
dc_trig_time(1,4)  -  (Default = -4.4) SC_P1 - DC_S4
dc_trig_time(1,5)  -  (Default =  0.0) SC_P1 - DC_S5
dc_trig_time(1,6)  -  (Default = -4.9) SC_P1 - DC_S6
